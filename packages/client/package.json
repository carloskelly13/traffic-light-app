{
  "name": "traffic-light-client",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "onoff": "^6.0.3",
    "tsup": "7.1.0",
    "typescript": "5.1.6"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node16",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/**/*.ts"
    ]
  },
  "devDependencies": {
    "@types/node": "20.4.5"
  }
}
